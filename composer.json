{
    "name": "aulasoftwarelibre/ddd-dev",
    "description": "Base dev classes for DDD projects",
    "authors": [
        {
            "name": "Sergio Gómez",
            "email": "sergio@uco.es"
        },
        {
            "name": "Víctor Monserrat",
            "email": "victormonserratvillatoro@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "prooph/event-sourcing": "^5.6",
        "symfony/http-kernel": "^4.1",
        "symfony/maker-bundle": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AulaSoftwareLibre\\DDD\\DevBundle\\": "src/"
        }
    }
}
